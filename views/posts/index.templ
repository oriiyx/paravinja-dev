package posts

import (
    "github.com/oriiyx/paravinja-dev/views/layout"
)

templ Index(blogPost string) {
    @layout.App() {
        <div class="container mx-auto my-10 px-4 sm:px-0">
            <article class="prose">
                @templ.Raw(blogPost)
            </article>
        </div>
    }
}